    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^4.13.0",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.0",
    "typescript": "^3.7.3",
    "url-loader": "^2.3.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
