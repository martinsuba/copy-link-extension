{
  "name": "copy-link-extension",
  "version": "1.0.0",
  "description": "Chrome extension for quick copying of currently opened website url, hovered anchor href or image source link by keyboard shortcuts.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && webpack --config ./build/webpack.config.js --mode=development",
    "build:production": "npm run clean && webpack --config ./build/webpack.config.js --mode=production",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.tsx,.ts",
    "clean": "del-cli dist"
  },
  "author": "Martin Suba",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@babel/runtime-corejs2": "^7.7.7",
    "@types/chrome": "0.0.91",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
